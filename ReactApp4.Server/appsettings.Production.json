{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Trace"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "WebApiDatabase": "Host=${DB_HOST};Database=${DB_NAME};Username=${DB_USER};Password=${DB_PASS}"
  },
  "Jwt": {
    "Key": "${JWT_KEY}",
    "Issuer": "${JWT_ISSUER}",
    "Audience": "${JWT_AUDIENCE}"
  },
  "AdminCredentials": {
    "Username": "${ADMIN_USER}",
    "Password": "${ADMIN_PASS}"
  }

}
